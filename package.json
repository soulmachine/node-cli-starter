{
  "name": "node-cli-starter",
  "version": "1.0.0",
  "description": "Node.js CLI project template",
  "main": "src/index.js",
  "bin": "src/index.js",
  "preferGlobal": true,
  "scripts": {
    "start": "node ./src/index.js",
    "lint": "eslint . --ext '.js,.jsx,.ts,.tsx,.md'",
    "prettier": "prettier -c --write '**/*'",
    "check-type": "tsc -p tsconfig.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/soulmachine/node-cli-starter.git"
  },
  "keywords": [
    "cli",
    "node",
    "starter"
  ],
  "author": "soulmachine",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/soulmachine/node-cli-starter/issues"
  },
  "homepage": "https://github.com/soulmachine/node-cli-starter",
  "dependencies": {
    "chalk": "^3.0.0",
    "figlet": "^1.2.4",
    "yargs": "^14.2.0"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/figlet": "^1.2.0",
    "@types/node": "^12.12.7",
    "@types/yargs": "^13.0.3",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.0.3",
    "eslint-plugin-markdown": "^1.0.1",
    "eslint-plugin-prettier": "^3.1.1",
    "prettier": "^1.19.1",
    "typescript": "^3.7.2"
  }
}
